    "webpack-merge": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@types/jasmine": "^2.8.8",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "jasmine": "^3.1.0",
    "karma": "^3.0.0",
    "karma-babel-preprocessor": "^8.0.0-beta.0",
    "karma-jasmine": "^1.1.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.0-beta.0",
    "phantomjs": "^2.1.7",
    "ts-loader": "^5.0.0",
    "typescript": "^3.0.3",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.5"
  }
